{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MongoDbConnectionOptions": {
    "ConnectionString": "MY_CONNECTION_STRING",
    "DatabaseName": "MY_DATABASE_NAME"
  },
  "JwtAuthenticationOptions": {
    "Issuer": "https://localhost:5001",
    "Audience": "https://localhost:5001",
    "Secret": "MY_SECRET",
    "ValidateSigningKey": true,
    "ExpirationMinutes": 180
  },
  "EmailOptions": {
    "Server": "YOUR_SMTP_SERVER",
    "Port": 587,
    "SenderName": "Your sender name",
    "SenderEmail": "Your sender email",
    "UserName": "Your username",
    "Password": "Your password"
  },
  "CorsOptions": {
    "AllowedOrigins": ["MY_ALLOWED_ORIGINS"]
  },
  "AdminUserOptions": {
    "_comment": "Optional options",
    "Username": "{EMAIL_OF_ADMIN}",
    "Password": "{PASSWORD_OF_ADMIN}",
    "FirstName": "{FIRST_NAME_OF_ADMIN}",
    "LastName": "{LAST_NAME_OF_ADMIN}",
    "HouseNumber": "{HOUSE_NUMBER_OF_ADMIN}",
    "DateOfBirth": "{DATE_OF_BIRTH_OF_ADMIN}"
  }
}
